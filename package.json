{
  "name": "mnb-transform-api",
  "version": "1.6.0",
  "private": true,
  "main": "dist/app.js",
  "repository": "https://github.com/MouseLightPipeline/mnb-transform-api.git",
  "author": "Patrick Edson <pedson@leapscientific.com>",
  "license": "SEE LICENSE IN LICENSE",
  "dockerRepository": "mouselightdatabrowser",
  "dockerImageName": "transform-api",
  "scripts": {
    "debug": "DEBUG=mnb:transform* node --trace-warnings --max-old-space-size=8192 --optimize-for-size src/app.js",
    "test-transform": "DEBUG=mnb:transform* node $NODE_DEBUG_OPTION examples/transformOnly.js",
    "docker-build": "gulp docker-build",
    "docker-release": "gulp release",
    "migrate": "tsc && ./migrate.sh"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(test|spec)\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "apollo-server-express": "^2.9.7",
    "archiver": "^3.1.1",
    "big-json": "^3.0.0",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "hdf5": "https://github.com/pedson/hdf5.node.git",
    "influx": "^5.5.0",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "sanitize-filename": "^1.6.3",
    "sequelize": "^5.21.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/archiver": "^3.0.0",
    "@types/bluebird": "^3.5.28",
    "@types/express": "^4.17.2",
    "@types/gulp": "4.0.6",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.12.6",
    "@types/sanitize-filename": "^1.6.3",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^10.11.3",
    "gulp": "^4.0.2",
    "gulp-shell": "^0.7.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^4.1.5"
  }
}
